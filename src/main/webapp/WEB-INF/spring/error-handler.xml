<?xml version="1.0" encoding="UTF-8"?>
<beans 	xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="error.handler.servlet-configbean" class="com.registeredreviews.utility.ErrorHandlerServletConfigImpl">
		<property name="emailToAddress" value="${errorhd_To}" />
		<property name="emailFromAddres" value="${errorhd_From}" />
		<property name="redirectPageDefault" value="${errorhd_Redirect}" />
		<property name="redirectPage404" value="${errorhd404_Redirect}" />
		<property name="redirectPage500" value="${errorhd500_Redirect}" />
	</bean>

</beans>
